{
  "name": "bot-telegram-pokemon",
  "version": "1.0.0",
  "description": "It's a pokemon bot",
  "main": "src/bot.js",
  "homepage": "https://github.com/darkusphantom/bot-pokemon",
  "scripts": {
    "start": "nodemon --exec babel-node src/bot.js",
    "start:example": "nodemon --exec babel-node src/bot-example.js",
    "start:ts": "ts-node --esm src/bot.ts",
    "dev": "nodemon --exec babel-node --require @babel/register src/bot.js",
    "build": "ncc build src/bot.js -o public -m",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "bot",
    "api",
    "nodejs",
    "javascript",
    "telegram"
  ],
  "author": "Luis Rodriguez (darkusphantom.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.23.8",
    "@types/node": "^20.11.0",
    "@vercel/ncc": "^0.38.1",
    "@vercel/node": "^3.0.16",
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "dotenv": "^16.0.3",
    "i18next": "^22.4.15",
    "telegraf": "^4.12.2"
  }
}
